{
  "title": "Pipeline Storage Commit",
  "uid": "pipeline-storage-commit",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "timezone": "browser",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Storage Stage Latency",
      "description": "Latency percentiles for storage commits.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.50, sum(rate(rpp_node_pipeline_stage_latency_ms_bucket{phase=\"storage\"}[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.95, sum(rate(rpp_node_pipeline_stage_latency_ms_bucket{phase=\"storage\"}[5m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "refId": "C",
          "expr": "histogram_quantile(0.99, sum(rate(rpp_node_pipeline_stage_latency_ms_bucket{phase=\"storage\"}[5m])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Storage Stage Throughput",
      "description": "Event throughput for storage commits.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(rpp_node_pipeline_stage_total{phase=\"storage\"}[5m]))",
          "legendFormat": "Storage"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Latest Commit Height",
      "description": "Gauge of the latest Firewood commit height reported by the pipeline.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 16 },
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max(rpp_node_pipeline_commit_height{phase=\"storage\"})"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      }
    }
  ]
}
