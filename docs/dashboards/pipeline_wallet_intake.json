{
  "title": "Pipeline Wallet Intake",
  "uid": "pipeline-wallet-intake",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "timezone": "browser",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "heatmap",
      "title": "Wallet Stage Latency Distribution",
      "description": "Latency distribution of wallet stage events.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(rpp_node_pipeline_stage_latency_ms_bucket{phase=\"wallet\"}[5m])) by (le)",
          "format": "time_series"
        }
      ],
      "options": {
        "legend": {
          "show": false
        },
        "tooltip": {
          "mode": "single"
        }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Wallet Stage Rate",
      "description": "Per-stage throughput, grouped by result labels.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 10 },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (result)(rate(rpp_node_pipeline_stage_total{phase=\"wallet\"}[5m]))",
          "legendFormat": "{{result}}"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Signer latency (p95)",
      "description": "Online/offline signing latency grouped by mode and account type.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le, mode, account_type) (rate(rpp_runtime_wallet_sign_latency_ms_bucket[5m])))",
          "legendFormat": "{{mode}}/{{account_type}}"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Signer failure rate", 
      "description": "Signing failure rate by backend, mode, and account type.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (mode, account_type, backend) (rate(rpp_runtime_wallet_sign_failures_total[5m]))",
          "legendFormat": "{{mode}}/{{account_type}}/{{backend}}"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    }
  ]
}
