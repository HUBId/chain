{
  "title": "Pipeline Overview",
  "uid": "pipeline-overview",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "timezone": "browser",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "heatmap",
      "title": "Wallet Intake Latency Heatmap",
      "description": "Distribution of wallet ingestion latency for the last 6 hours.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(rpp_node_pipeline_stage_latency_ms_bucket{phase=\"wallet\"}[5m])) by (le)",
          "format": "time_series"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "single"
        },
        "legend": {
          "show": false
        }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Wallet Intake Throughput",
      "description": "Pipeline wallet stage throughput sampled over 5 minutes.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(rpp_node_pipeline_stage_total{phase=\"wallet\"}[5m]))",
          "legendFormat": "Wallet"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Proof Validation Latency (p95)",
      "description": "95th percentile proof validation latency.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(rpp_node_pipeline_stage_latency_ms_bucket{phase=\"proof\"}[5m])) by (le))",
          "legendFormat": "Proof p95"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Consensus vs Storage Throughput",
      "description": "Comparative throughput of consensus and storage stages.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(rpp_node_pipeline_stage_total{phase=\"consensus\"}[5m]))",
          "legendFormat": "Consensus"
        },
        {
          "refId": "B",
          "expr": "sum(rate(rpp_node_pipeline_stage_total{phase=\"storage\"}[5m]))",
          "legendFormat": "Storage"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Storage Commit Height",
      "description": "Latest committed height observed by the storage stage.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 6, "w": 24, "x": 0, "y": 17 },
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "max(rpp_node_pipeline_commit_height{phase=\"storage\"})",
          "legendFormat": "Commit height"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "right"
        }
      }
    }
  ]
}
