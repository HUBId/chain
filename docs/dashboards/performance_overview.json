{
  "title": "Performance Benchmark Overview",
  "uid": "performance-overview",
  "schemaVersion": 39,
  "version": 1,
  "timezone": "browser",
  "refresh": "1m",
  "time": {
    "from": "now-7d",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Scenario throughput (TPS)",
      "description": "Rolling throughput for each benchmark scenario using JSON exporter samples ingested via the performance collector.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (scenario) (firewood_benchmark_throughput_tps)",
          "legendFormat": "{{scenario}}"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "p99 commit latency",
      "description": "Latest percentile samples emitted by scripts/ci/run_benchmarks.sh and scraped into Prometheus via the JSON performance exporter.",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "gridPos": { "h": 9, "w": 24, "x": 0, "y": 9 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "targets": [
        {
          "refId": "A",
          "expr": "firewood_benchmark_latency_ms{quantile=\"0.99\"}",
          "legendFormat": "{{scenario}}"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    }
  ]
}
