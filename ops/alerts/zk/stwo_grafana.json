{
  "title": "STWO Prover SLA",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "p95 latency (ms)",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 },
      "options": {
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(rpp_runtime_wallet_prover_job_duration_ms_bucket{backend=\"stwo\"}[10m])) by (le, backend))",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 120000 },
              { "color": "red", "value": 180000 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Throughput (proofs/sec)",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 0 },
      "options": {
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "expr": "sum(rate(rpp_runtime_wallet_prover_jobs_total{backend=\"stwo\",result=\"success\"}[5m]))",
          "legendFormat": "success"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 0.8 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Failure ratio",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 0 },
      "options": {
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "expr": "increase(rpp_runtime_wallet_prover_jobs_total{backend=\"stwo\",result=\"failure\"}[15m]) / clamp_min(increase(rpp_runtime_wallet_prover_jobs_total{backend=\"stwo\"}[15m]), 1)",
          "legendFormat": "failure_ratio"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.1 },
              { "color": "red", "value": 0.2 }
            ]
          }
        },
        "overrides": []
      }
    }
  ]
}
